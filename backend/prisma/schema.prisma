generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url=env(DATABASE_URL)
 
}

model User {
  id        Int      @id @default(autoincrement())
  name      String?
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}

model Incident {
  id        Int      @id @default(autoincrement())
  type      String
  latitude  Float
  longitude Float
  address   String?
  time      DateTime
  createdAt DateTime @default(now())
}
